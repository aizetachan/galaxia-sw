{
  "version": 2,
  "functions": {
    "health.js": {
      "runtime": "nodejs18.x"
    },
    "auth.js": {
      "runtime": "nodejs18.x"
    },
    "[...all].js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "/api/health", "dest": "/health.js" },
    { "src": "/api/auth/(.*)", "dest": "/auth.js" },
    { "src": "/api/(.*)", "dest": "/[...all].js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "outputDirectory": "dist"
}